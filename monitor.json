{
  "watch_directory": "./incoming",
  "output_directory": "./output",
  "poll_interval": 5,
  "delete_input": true,
  "log_level": "INFO",
  "rules": [
    {
      "pattern": "*_live.mov",
      "profile": "live-qlab"
    },
    {
      "pattern": "archive_*.mov", 
      "profile": "archival"
    },
    {
      "pattern": "*.mov",
      "profile": "standard-playback"
    }
  ]
}
